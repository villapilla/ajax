# Do not remove this line or mod_rewrite rules and search engine friendly URLs will stop working
RewriteBase /
RewriteEngine On
RewriteRule ^buscar/(.*)/product?$ buscar/index.php?search=$1  [NC,L]
RewriteRule ^description/([0-9]+)?$ description/index.php?search=$1  [NC,L]
RewriteRule ^dataset/(.*)/product?$ dataset/index.php?search=$1  [NC,L]